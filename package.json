{
  "name": "apps-script-jobqueue",
  "version": "v0.1.7",
  "description": "A simple job queue library for Google Apps Script.",
  "type": "module",
  "private": true,
  "main": "src/index.ts",
  "typings": "src/index.d.ts",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "deploy": "npm run build && clasp push -f",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint --fix src/**/*.ts __tests__/**/*.ts",
    "lint:check": "eslint src/**/*.ts __tests__/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"__tests__/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k2tzumi/apps-script-jobqueue.git"
  },
  "author": "k2tzumi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/k2tzumi/apps-script-jobqueue/issues"
  },
  "homepage": "https://github.com/k2tzumi/apps-script-jobqueue#readme",
  "devDependencies": {
    "@google/clasp": "^3.1.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "globals": "^17.1.0",
    "jest": "^29.4.3",
    "prettier": "^3.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.0.5",
    "tslib": "^2.6.2",
    "typescript": "~5.5.0"
  },
  "dependencies": {
    "@types/google-apps-script": "^1.0.57"
  }
}
