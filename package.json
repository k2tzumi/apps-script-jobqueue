{
  "name": "apps-script-jobqueue",
  "version": "v0.1.10",
  "description": "A simple job queue library for Google Apps Script.",
  "type": "module",
  "private": true,
  "main": "src/index.ts",
  "typings": "src/index.d.ts",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "deploy": "npm run build && clasp push -f",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint --fix src/**/*.ts __tests__/**/*.ts",
    "lint:check": "eslint src/**/*.ts __tests__/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"__tests__/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k2tzumi/apps-script-jobqueue.git"
  },
  "author": "k2tzumi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/k2tzumi/apps-script-jobqueue/issues"
  },
  "homepage": "https://github.com/k2tzumi/apps-script-jobqueue#readme",
  "devDependencies": {
    "@google/clasp": "^3.1.3",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.1.0",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "rollup": "^4.57.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "@types/google-apps-script": "^2.0.8"
  }
}
